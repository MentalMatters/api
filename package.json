{
	"name": "mental-health",
	"version": "1.0.50",
	"description": "An API designed to make mental health support accessible to everyone. Available at https://api-mentalmatters.tdanks.com/",
	"repository": {
		"type": "git",
		"url": "https://github.com/MentalMatters/mentalmatters-api"
	},
	"scripts": {
		"dev": "bun run --watch src/index.ts",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"db:seed": "ts-node --esm ./src/server/db/seed"
	},
	"dependencies": {
		"@elysiajs/cors": "^1.3.3",
		"drizzle-orm": "^0.43.1",
		"elysia": "latest",
		"ioredis": "^5.6.1",
		"node-cron": "^4.0.6",
		"uuid": "^11.1.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.0-beta.5",
		"bun-types": "latest",
		"drizzle-kit": "^0.31.1"
	},
	"module": "src/index.js"
}
